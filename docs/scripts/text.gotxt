{{- range .TextBlocks -}}
	{{- if eq .Kind "plain" -}}
		{{- .Text -}}
	{{- else if eq .Kind "italic" -}}
		{{- .Text -}}
	{{- else if eq .Kind "link" -}}
		{{- link (exec "text" .Inner) .Href -}}
	{{- else if eq .Kind "docLink" -}}
		{{- docLink (exec "text" .Inner) (printf "%s/API" .Path) (localHref .Href) -}}
	{{- end -}}
{{- end -}}

